{
  "name": "jabdb",
  "version": "0.1.0",
  "description": "Just A Basic Database",
  "main": "dist/src/JabDB.js",
  "repository": "https://github.com/jollycola/jabDB.git",
  "author": "Mathias Andresen <dev@andresen.xyz>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run build-ts",
    "rebuild": "npm run clean && npm run build",
    "watch": "tsc --watch",
    "build-ts": "tsc -p tsconfig.json",
    "tslint": "tslint -p tsconfig.json",
    "build-and-test": "yarn build && yarn test",
    "test": "mocha -r ts-node/register test/**/*.ts --recursive",
    "coverage": "nyc yarn test",
    "docs": "typedoc --out docs --target es6 --theme minimal --mode file src"
  },
  "private": true,
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@microsoft/tsdoc": "^0.12.14",
    "@types/chai": "^4.2.2",
    "@types/chai-as-promised": "^7.1.2",
    "@types/lodash": "^4.14.138",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3",
    "typedoc-plugin-nojekyll": "^1.0.1"
  }
}
